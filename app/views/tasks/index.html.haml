.search
  = form_tag tasks_url, method: :get do
    = text_field_tag :search, params[:search]
    = submit_tag "Search"
%h1 Check List
= link_to "New Task", new_task_path, id: "new_link"
%h2 Incomplete Tasks
#incomplete_tasks.tasks
  = render @incomplete_tasks
%h2 Comlpete Tasks
#complete_tasks.tasks
  = render @complete_tasks
